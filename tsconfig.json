{
  "compilerOptions": {
      "target": "ES2020",
      "rootDir": "./",
      "outDir": "./dist/",
      "baseUrl": "./src/",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "module": "CommonJS",
      "esModuleInterop": true,
      "skipLibCheck": true,
  },
  "exclude": ["node_modules"],
}